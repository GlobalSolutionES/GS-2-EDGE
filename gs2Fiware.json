							},
							{
								"key": "fiware-servicepath",
								"value": "/",
								"type": "text"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"devices\": [\n    {\n      \"device_id\": \"workfuture001\",         // Identificador único do dispositivo\n      \"entity_name\": \"urn:ngsi-ld:WorkFuture:001\",   // Nome da entidade para o dispositivo (formato URN)\n      \"entity_type\": \"WorkFuture\",          // Tipo da entidade (neste caso, \"WorkFuture\")\n      \"protocol\": \"PDI-IoTA-UltraLight\",  // Protocolo usado para a comunicação (no exemplo, PDI-IoTA-UltraLight)\n      \"transport\": \"MQTT\",            // Método de transporte (neste caso, MQTT)\n\n      // Lista de comandos que o dispositivo pode receber\n      \"commands\": [\n        { \"name\": \"system_on\", \"type\": \"command\" },  // Comando para ligar sistema\n        { \"name\": \"system_off\", \"type\": \"command\" }  // Comando para desligar sistema\n      ],\n\n      // Lista de atributos associados ao dispositivo\n      \"attributes\": [\n        { \"object_id\": \"temperature\", \"name\": \"temperature\", \"type\": \"Float\" }, // Atributo de temperatura\n        { \"object_id\": \"humidity\", \"name\": \"humidity\", \"type\": \"Float\" }, // Atributo de umidade\n        { \"object_id\": \"light\", \"name\": \"light\", \"type\": \"Integer\" }, // Atributo de luminosidade\n        { \"object_id\": \"presence\", \"name\": \"presence\", \"type\": \"Boolean\" }, // Atributo de presença\n        { \"object_id\": \"noise_level\", \"name\": \"noise_level\", \"type\": \"Integer\" }, // Atributo de nível de ruído\n        { \"object_id\": \"energy_efficiency\", \"name\": \"energy_efficiency\", \"type\": \"Float\" } // Atributo de eficiência energética\n      ]\n    }\n  ]\n}"
						},
						"url": {
							"raw": "http://{{url}}:4041/iot/devices",
							"protocol": "http",
							"host": [
								"{{url}}"
							],
							"port": "4041",
							"path": [
								"iot",
								"devices"
							]
						}
					},
					"response": []
				},
